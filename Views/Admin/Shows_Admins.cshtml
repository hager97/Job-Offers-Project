@model IEnumerable<jobs.Models.ApplicationUser>

@{
    ViewBag.Title = "Shows_Admins";
    Layout = "~/Views/Shared/_MainLayoutPage.cshtml";
}

<h2>Shows_Admins</h2>

<p>
    @Html.ActionLink("اضافة ادمن", "Add_Admin")
</p>
<table class="table">
    <tr>
